{
  "name": "monorepo-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint:format": "turbo lint:format",
    "lint:lint": "turbo lint:lint",
    "lint:spellcheck": "cspell lint \"(packages|apps)/**/*.{js,ts,mjs,cjs,json,css,less,scss,vue,html,md}\"",
    "lint:depcheck": "depcheck",
    "lint:commit": "commitlint --edit $1",
    "typecheck": "turbo typecheck",
    "commit": "git-cz",
    "prepare": "husky",
    "precommit": "lint-staged",
    "dev:web": "turbo build --filter !@monorepo-example/web && turbo watch dev --filter @monorepo-example/web",
    "dev:desktop": "turbo build --filter !@monorepo-example/desktop && turbo watch dev --filter @monorepo-example/desktop",
    "dev:backend": "turbo dev --filter @monorepo-example/backend"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "keywords": [],
  "author": "Coke <jock74608@gmail.com>",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=24.11.0",
    "npm": ">=10.9.2",
    "pnpm": ">=10.20.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@cspell/dict-python": "^4.2.22",
    "@cspell/dict-typescript": "^3.2.3",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "commitizen": "^4.3.1",
    "cspell": "^9.3.0",
    "cz-git": "^1.12.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vue": "^3.5.24",
    "vue-eslint-parser": "^10.2.0"
  }
}
